():\n","    heart_rate = heart_rate_entry.get()\n","    step_number = step_number_entry.get()\n","    sleep_data = sleep_data_entry.get()\n","    \n","    # Do something with the data (e.g. store it in a database)\n","    print(\"Heart Rate:\", heart_rate)\n","    print(\"Step Number:\", step_number)\n","    print(\"Sleep Data:\", sleep_data)\n","\n","# Bind the button to the submit_data function\n","submit_button.config(command=submit_data)\n","\n","# Run the main loop\n","root.mainloop()\n"],"metadata":{"id":"23KLFZrJPkjc"},"execution_count":null,"outputs":[]}]}